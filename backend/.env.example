# 服务器配置
PORT=8000
NODE_ENV=development

# 安全配置
JWT_SECRET=your_jwt_secret_here
JWT_EXPIRES_IN=1d

# 数据库配置
MONGODB_URI=mongodb://localhost:27017/education_ai

# AI服务配置
AI_PROVIDER=qwen3
AI_API_KEY=your_api_key_here
AI_ENDPOINT=https://dashscope.aliyuncs.com/compatible-mode/v1
AI_MODEL=qwen-plus
AI_TIMEOUT=30000
AI_MAX_RETRIES=3
AI_TEMPERATURE=0.7
AI_MAX_TOKENS=2000

# 开源模型配置（可选）
OPEN_SOURCE_ENDPOINT=http://localhost:8000
OPEN_SOURCE_MODEL=qwen-coder